    {
      "parameters": {
        "method": "POST",
        "url": "={{ 'https://googleads.googleapis.com/v11/customers/' + $json[\"Client_Id\"] + '/googleAds:search' }}",
        "authentication": "genericCredentialType",
        "genericAuthType": "googleAdsOAuth2Api",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "developer-token",
              "value": "={{ $json[\"developer_token\"] }}"
            },
            {
              "name": "login-customer-id",
              "value": "={{ $json[\"Manager_Id\"] }}"
            },
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        },
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={{ { \"query\": \"SELECT segments.date, ad_group_ad.ad.id, segments.conversion_action_category, metrics.conversions FROM ad_group_ad WHERE segments.date DURING LAST_90_DAYS AND campaign.name LIKE '%\" + $json[\\\"campaign_name_filter\\\"] + \"%' AND segments.conversion_action_category IN ('LEAD','PURCHASE')\" } }}",
        "options": {
          "splitIntoItems": true,
          "propertyName": "results"
        }
      },
      "name": "Get Conversions (Lead & Purchase)",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        440,
        200
      ],
      "id": "get-conv-node",
      "credentials": {
        "googleAdsOAuth2Api": {
          "id": "",
          "name": "Google Ads technology@pedroadao"
        }
      }
    },
    {
      "parameters": {
        "functionCode": "const grouped = {};\nfor (const item of items) {\n  const r = item.json;\n  const adId = r.adGroupAd.ad.id;\n  const date = r.segments.date;\n  const category = r.segments.conversionActionCategory;\n  const conversions = Number(r.metrics.conversions || 0);\n  const key = `${adId}-${date}`;\n  if (!grouped[key]) {\n    grouped[key] = {\n      ad_id: adId,\n      date,\n      reported_leads: 0,\n      reported_purchases: 0\n    };\n  }\n  if (category === 'LEAD') grouped[key].reported_leads += conversions;\n  if (category === 'PURCHASE') grouped[key].reported_purchases += conversions;\n}\nreturn Object.values(grouped).map(r => ({ json: r }));"
      },
      "type": "n8n-nodes-base.function",
      "typeVersion": 2,
      "position": [
        700,
        200
      ],
      "id": "aggregate-node",
      "name": "Aggregate Conversions"
    },
    {
      "parameters": {
        "functionCode": "const perfItems = $items(0);\nconst convItems = $items(1);\n\nconst convIndex = {};\nfor (const item of convItems) {\n  const r = item.json;\n  const key = `${r.ad_id}-${r.date}`;\n  convIndex[key] = r;\n}\n\nconst out = [];\nfor (const item of perfItems) {\n  const r = item.json;\n  const adId = r.adGroupAd.ad.id;\n  const date = r.segments.date;\n  const key = `${adId}-${date}`;\n  const conv = convIndex[key] || { reported_leads: 0, reported_purchases: 0 };\n\n  const costMicros = Number(r.metrics.costMicros || 0);\n  const spend = costMicros / 1000000;\n\n  out.push({\n    date,\n    platform: 'google',\n    spend,\n    clicks: Number(r.metrics.clicks || 0),\n    impressions: Number(r.metrics.impressions || 0),\n    link_clicks: Number(r.metrics.clicks || 0),\n    reported_leads: conv.reported_leads,\n    reported_purchases: conv.reported_purchases,\n    adset_id: r.adGroup.id,\n    adset_name: r.adGroup.name,\n    ad_id: adId,\n    ad_name: r.adGroupAd.ad.name,\n    inline_link_clicks: Number(r.metrics.clicks || 0),\n    landing_page_view_rate: null,\n    campaign_name: r.campaign.name,\n    campaign_id: r.campaign.id\n  });\n}\n\nreturn [{ json: out }];"
      },
      "type": "n8n-nodes-base.function",
      "typeVersion": 2,
      "position": [
