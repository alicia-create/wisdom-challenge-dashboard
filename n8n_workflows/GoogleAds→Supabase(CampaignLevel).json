    {
      "parameters": {
        "jsCode": "// Transform Google Ads campaign data to Supabase format\nconst items = $input.all();\nconst campaignFilter = $('Data').first().json.campaign_name_filter;\nconst transformed = [];\n\nfor (const item of items) {\n  try {\n    const campaign = item.json;\n    \n    // Filter by campaign name\n    if (!campaign.name || !campaign.name.includes(campaignFilter)) {\n      continue;\n    }\n    \n    // Extract date from period (format: DAILY)\n    // If period is DAILY, use today's date\n    const date = new Date().toISOString().split('T')[0];\n    \n    // Convert costMicros to spend\n    const costMicros = Number(campaign.costMicros || 0);\n    const spend = costMicros / 1000000;\n    \n    transformed.push({\n      json: {\n        platform: 'google',\n        date: date,\n        campaign_id: campaign.id,\n        campaign_name: campaign.name,\n        adset_id: null,\n        adset_name: null,\n        ad_id: null,\n        ad_name: null,\n        spend: spend,\n        clicks: Number(campaign.interactions || 0),\n        impressions: Number(campaign.impressions || 0),\n        link_clicks: Number(campaign.interactions || 0),\n        inline_link_clicks: Number(campaign.interactions || 0),\n        landing_page_view_per_link_click: null,\n        reported_leads: Number(campaign.conversions || 0),\n        reported_purchases: 0\n      }\n    });\n  } catch (error) {\n    console.error('Error transforming campaign:', error);\n    continue;\n  }\n}\n\nreturn transformed;"
      },
      "name": "Transform to Supabase Format",
      "type": "n8n-nodes-base.code",
